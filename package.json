{
  "name": "dynastar",
  "version": "2.0.0",
  "description": "A simple compatibility layer for dynamodb models to be compatible with the datastar model API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.4",
    "@types/lodash": "^4.14.195",
    "assume": "^2.2.0",
    "assume-sinon": "^1.0.1",
    "async": "^3.1.0",
    "aws-liveness": "^1.1.0",
    "aws-sdk": "^2.557.0",
    "dynamodb-x": "^2.0.0",
    "eslint": "^7.9.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "joi": "^17.2.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "test": "nyc npm run mocha",
    "mocha": "mocha test/*.test.js",
    "lint": "eslint-godaddy --fix *.js test/*.js",
    "posttest": "npm run lint",
    "build": "tsc -P tsconfig.build.json",
    "prepublishOnly": "npm run test && npm run build",
    "localstack": "docker run -it -p 4566:4566 -e SERVICES=dynamodb --rm localstack/localstack:0.12.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/dynastar.git"
  },
  "keywords": [
    "dynamodb",
    "datastar"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT"
}
